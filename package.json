{
  "name": "graasp-file-upload-limiter",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "File Upload Limiter fastify plugin for Graasp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build & yarn hooks:install",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"**/*.ts\"",
    "prettier:write": "prettier --write \"**/*.ts\"",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status",
    "test": "jest  --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/eslint": "^8.2.2",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "fastify": "^3.25.3",
    "graasp-test": "github:graasp/graasp-test",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@graasp/translations": "github:graasp/graasp-translations",
    "http-status-codes": "2.2.0",
    "slonik": "^27.1.0"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
